{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:import/recommended"
  ],
  "plugins": [
    "import"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ]
  }
}
