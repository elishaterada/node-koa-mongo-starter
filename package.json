{
  "name": "@elishaterada/node-starter",
  "version": "0.1.0",
  "description": "A boilerplate for Node projects",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "node_modules/.bin/nodemon --exec npm run restart",
    "restart": "node_modules/.bin/rimraf dist && npm run build && npm start",
    "start": "npx node dist/index.js",
    "lint": "node_modules/.bin/eslint --quiet **/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elishaterada/node-starter.git"
  },
  "keywords": [
    "Node"
  ],
  "author": "Elisha Terada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elishaterada/node-starter/issues"
  },
  "homepage": "https://github.com/elishaterada/node-starter#readme",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "bcrypt": "^3.0.4",
    "jsonwebtoken": "^8.5.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-passport": "^4.1.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.15",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "mongodb-memory-server": "^3.1.1",
    "nodemon": "^1.18.3",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3"
  }
}
